<table class="table table-bordered">
    <thead>
        <tr>
        <th>Title</th>
        <th>Text</th>
        <th>User id</th>
        </tr>
    </thead>
    <tbody>
        <% posts.forEach(post=> { %>
            <tr>
            <td><%= post.title %></td>
            <td><%= post.text %></td>
            <td><%= post.userID %></td>
        </tr>
        <% })%>
    </tbody>
</table>